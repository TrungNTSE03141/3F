    @*TrungNT Init Google map*@
    <script>
        function InitMap() {
            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 13,
                center: new google.maps.LatLng(@Model.latitude, @Model.longtitude),
                mapTypeId: google.maps.MapTypeId.ROADMAP
            });

            var infowindow = new google.maps.InfoWindow({});

            var marker;

            marker = new google.maps.Marker({
                position: new google.maps.LatLng(@Model.latitude, @Model.longtitude),
                map: map
            });
        }
    </script>

